{
  "name": "webditor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev-windows": "concurrently \"pnpm run fastapi-dev-windows\" \"pnpm run next-dev\"",
    "dev-linux": "concurrently \"pnpm run fastapi-dev-linux\" \"pnpm run next-dev\"",
    "next-dev": "cd frontend && pnpm run dev",
    "fastapi-dev-windows": "./backend/.venv/Scripts/activate && cd backend && uvicorn app.main:app --reload --port 8000 --log-level debug --reload-dir ../wengine",
    "fastapi-dev-linux": "source ./backend/.venv/bin/activate && cd backend && uvicorn app.main:app --reload --port 8000 --log-level debug --reload-dir ../wengine",
    "build-windows": "./preprocess/.venv/Scripts/activate && python scripts/build.py",
    "build-linux": "source ./preprocess/.venv/bin/activate && python scripts/build.py"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.1.2",
    "json-schema-to-typescript": "^15.0.4"
  },
  "devDependencies": {
    "json-refs": "^3.0.15",
    "json-schema-to-zod": "^2.6.1",
    "prettier": "^3.5.3",
    "quicktype": "^23.0.171"
  }
}
